local library = loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/ShaddowScripts/Main/main/Library"))()

local Main = library:CreateWindow("[UPDü¶ë] +1 Speed Prison Escape","Crimson")

local tab = Main:CreateTab("Cheats")

local UserInputService = game:GetService("UserInputService")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

local isInfinityJumpEnabled = false
local jumpConnection = nil

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è Infinity Jump
local function enableInfinityJump()
    if jumpConnection then return end -- –ï—Å–ª–∏ —É–∂–µ –ø–æ–¥–∫–ª—é—á–µ–Ω–æ, –Ω–∏—á–µ–≥–æ –Ω–µ –¥–µ–ª–∞–µ–º

    jumpConnection = UserInputService.JumpRequest:Connect(function()
        if humanoid and humanoid.Health > 0 then
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end)
    print("Infinity Jump –≤–∫–ª—é—á–µ–Ω")
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤—ã–∫–ª—é—á–µ–Ω–∏—è Infinity Jump
local function disableInfinityJump()
    if jumpConnection then
        jumpConnection:Disconnect()
        jumpConnection = nil
    end
    print("Infinity Jump –≤—ã–∫–ª—é—á–µ–Ω")
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏—è Infinity Jump
local function toggleInfinityJump(enabled)
    isInfinityJumpEnabled = enabled
    if enabled then
        enableInfinityJump()
    else
        disableInfinityJump()
    end
end

-- UI —ç–ª–µ–º–µ–Ω—Ç (Toggle)
tab:CreateToggle("Infinity Jump", function(enabled)
    toggleInfinityJump(enabled)
end)

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –ø–µ—Ä—Å–æ–Ω–∞–∂–∞ (–Ω–∞ —Å–ª—É—á–∞–π —Ä–µ—Å–ø–∞–≤–Ω–∞)
player.CharacterAdded:Connect(function(char)
    character = char
    humanoid = character:FindFirstChild("Humanoid")
    if isInfinityJumpEnabled then
        enableInfinityJump()
    end
end)

-- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –ø—Ä–∏ —Å—Ç–∞—Ä—Ç–µ
if character then
    humanoid = character:FindFirstChild("Humanoid")
end

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")

-- –ü–æ–ª—É—á–∞–µ–º RemoteFunction
local OpenEggFunction = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("RemoteFunctions"):WaitForChild("OpenEgg")

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∏ –∞–≤—Ç–æ –æ—Ç–∫—Ä—ã—Ç–∏—è —è–π—Ü–∞
local autoEggEnabled = false
local eggInterval = 0.1 -- –ò–Ω—Ç–µ—Ä–≤–∞–ª –º–µ–∂–¥—É –æ—Ç–∫—Ä—ã—Ç–∏—è–º–∏ —è–∏—Ü (–≤ —Å–µ–∫—É–Ω–¥–∞—Ö)
local lastEggTime = 0
local selectedEgg = "Summon1" -- –Ø–π—Ü–æ –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è/–≤—ã–∫–ª—é—á–µ–Ω–∏—è –∞–≤—Ç–æ –æ—Ç–∫—Ä—ã—Ç–∏—è —è–π—Ü–∞
local function toggleAutoEgg(enabled)
    autoEggEnabled = enabled
    if autoEggEnabled then
        print("–ê–≤—Ç–æ –æ—Ç–∫—Ä—ã—Ç–∏–µ —è–∏—Ü –≤–∫–ª—é—á–µ–Ω–æ")
    else
        print("–ê–≤—Ç–æ –æ—Ç–∫—Ä—ã—Ç–∏–µ —è–∏—Ü –≤—ã–∫–ª—é—á–µ–Ω–æ")
    end
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤—ã–±–æ—Ä–∞ —è–π—Ü–∞
local function selectEgg(eggName)
    selectedEgg = eggName
    print("–í—ã–±—Ä–∞–Ω–æ —è–π—Ü–æ: " .. eggName)
end

-- UI —ç–ª–µ–º–µ–Ω—Ç (Dropdown)
tab:CreateDropdown("Choose Egg", {"Summon1", "Summon2", "Summon3"}, function(eggName)
    selectEgg(eggName) -- –í—ã–∑—ã–≤–∞–µ–º —Ñ—É–Ω–∫—Ü–∏—é –ø—Ä–∏ –∏–∑–º–µ–Ω–µ–Ω–∏–∏ Dropdown
end)

-- UI —ç–ª–µ–º–µ–Ω—Ç (Toggle)
tab:CreateToggle("Auto Egg", function(enabled)
    toggleAutoEgg(enabled) -- –í—ã–∑—ã–≤–∞–µ–º —Ñ—É–Ω–∫—Ü–∏—é –ø—Ä–∏ –∏–∑–º–µ–Ω–µ–Ω–∏–∏ Toggle
end)

-- –û—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª
RunService.Heartbeat:Connect(function()
    if autoEggEnabled then
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –≤—Ä–µ–º—è —Å –ø–æ—Å–ª–µ–¥–Ω–µ–≥–æ –æ—Ç–∫—Ä—ã—Ç–∏—è
        if tick() - lastEggTime >= eggInterval then
            lastEggTime = tick()

            -- –í—ã–∑—ã–≤–∞–µ–º RemoteFunction OpenEgg
            local success, result = pcall(function()
                return OpenEggFunction:InvokeServer(selectedEgg)
            end)

            -- –û–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º —Ä–µ–∑—É–ª—å—Ç–∞—Ç
            if success then
                print("–Ø–π—Ü–æ –æ—Ç–∫—Ä—ã—Ç–æ, —Ä–µ–∑—É–ª—å—Ç–∞—Ç: " .. tostring(result))
            else
                warn("–û—à–∏–±–∫–∞ –ø—Ä–∏ –æ—Ç–∫—Ä—ã—Ç–∏–∏ —è–π—Ü–∞: " .. tostring(result))
                toggleAutoEgg(false) -- –û—Ç–∫–ª—é—á–∞–µ–º –∞–≤—Ç–æ–æ—Ç–∫—Ä—ã—Ç–∏–µ –ø—Ä–∏ –æ—à–∏–±–∫–µ
            end
        end
    end
end)

local DupeEnabled = false  -- –ò–∑–Ω–∞—á–∞–ª—å–Ω–æ –≤—ã–∫–ª—é—á–µ–Ω

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∑–∞–ø—É—Å–∫–∞ —Å–∫—Ä–∏–ø—Ç–∞
local function startDupe()
    spawn(function()
        while DupeEnabled do
            -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –∂–∏–≤ –ª–∏ –µ—â–µ –ø–µ—Ä—Å–æ–Ω–∞–∂
            if not game.Players.LocalPlayer.Character or not game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                wait(1) -- –ñ–¥–µ–º, –ø–æ–∫–∞ –ø–µ—Ä—Å–æ–Ω–∞–∂ –ø–æ—è–≤–∏—Ç—Å—è
            else
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Chechpoint_Texts.Start.CFrame
                wait(0.1)
                for i, v in pairs(workspace.Mainbuild.Levels.Finish.FinishBeams:GetChildren()) do
                    if v:IsA("BasePart") then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                        wait(0.05)
                    end
                end
                wait(0.5)
            end
        end
    end)
end

-- –§—É–Ω–∫—Ü–∏—è, –∫–æ—Ç–æ—Ä–∞—è –±—É–¥–µ—Ç –≤—ã–∑—ã–≤–∞—Ç—å—Å—è –ø—Ä–∏ –∏–∑–º–µ–Ω–µ–Ω–∏–∏ Toggle
local function onToggleChanged(newValue)
    DupeEnabled = newValue
    if DupeEnabled then
        print("Dupe —Å–∫—Ä–∏–ø—Ç –≤–∫–ª—é—á–µ–Ω")
        startDupe()  -- –ó–∞–ø—É—Å–∫–∞–µ–º, —Ç–æ–ª—å–∫–æ –µ—Å–ª–∏ –≤–∫–ª—é—á–∞–µ–º
    else
        print("Dupe —Å–∫—Ä–∏–ø—Ç –≤—ã–∫–ª—é—á–µ–Ω")
        -- –ù–∏—á–µ–≥–æ –Ω–µ –¥–µ–ª–∞–µ–º, —Ü–∏–∫–ª —Å–∞–º –æ—Å—Ç–∞–Ω–æ–≤–∏—Ç—Å—è
    end
end

-- –°–æ–∑–¥–∞–µ–º Toggle
tab:CreateToggle("Dupe Wins", function(a)
    onToggleChanged(a)
end)

print("Dupe —Å–∫—Ä–∏–ø—Ç –∑–∞–≥—Ä—É–∂–µ–Ω, –∏—Å–ø–æ–ª—å–∑—É–π—Ç–µ Toggle –¥–ª—è —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è.")

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")

-- –ü–æ–ª—É—á–∞–µ–º RemoteFunction
local EquipBestFunction = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("RemoteFunctions"):WaitForChild("EquipBest")

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∏ –∞–≤—Ç–æ —ç–∫–∏–ø–∏—Ä–æ–≤–∫–∏
local autoEquipEnabled = false
local equipInterval = 0.1 -- –ò–Ω—Ç–µ—Ä–≤–∞–ª –º–µ–∂–¥—É —ç–∫–∏–ø–∏—Ä–æ–≤–∫–∞–º–∏ (–≤ —Å–µ–∫—É–Ω–¥–∞—Ö)
local lastEquipTime = 0

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è/–≤—ã–∫–ª—é—á–µ–Ω–∏—è –∞–≤—Ç–æ —ç–∫–∏–ø–∏—Ä–æ–≤–∫–∏
local function toggleAutoEquip(enabled)
    autoEquipEnabled = enabled
    if autoEquipEnabled then
        print("–ê–≤—Ç–æ —ç–∫–∏–ø–∏—Ä–æ–≤–∫–∞ –≤–∫–ª—é—á–µ–Ω–∞")
    else
        print("–ê–≤—Ç–æ —ç–∫–∏–ø–∏—Ä–æ–≤–∫–∞ –≤—ã–∫–ª—é—á–µ–Ω–∞")
    end
end

-- UI —ç–ª–µ–º–µ–Ω—Ç (Toggle)
tab:CreateToggle("Auto Equip Best", function(enabled)
    toggleAutoEquip(enabled) -- –í—ã–∑—ã–≤–∞–µ–º —Ñ—É–Ω–∫—Ü–∏—é –ø—Ä–∏ –∏–∑–º–µ–Ω–µ–Ω–∏–∏ Toggle
end)

-- –û—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª
RunService.Heartbeat:Connect(function()
    if autoEquipEnabled then
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –≤—Ä–µ–º—è —Å –ø–æ—Å–ª–µ–¥–Ω–µ–π —ç–∫–∏–ø–∏—Ä–æ–≤–∫–∏
        if tick() - lastEquipTime >= equipInterval then
            lastEquipTime = tick()

            -- –í—ã–∑—ã–≤–∞–µ–º RemoteFunction EquipBest
            local success, result = pcall(function()
                return EquipBestFunction:InvokeServer()
            end)

            -- –û–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º —Ä–µ–∑—É–ª—å—Ç–∞—Ç
            if success then
                print("–õ—É—á—à–∏–π –ø—Ä–µ–¥–º–µ—Ç —ç–∫–∏–ø–∏—Ä–æ–≤–∞–Ω, —Ä–µ–∑—É–ª—å—Ç–∞—Ç: " .. tostring(result))
            else
                warn("–û—à–∏–±–∫–∞ –ø—Ä–∏ —ç–∫–∏–ø–∏—Ä–æ–≤–∫–µ –ª—É—á—à–µ–≥–æ –ø—Ä–µ–¥–º–µ—Ç–∞: " .. tostring(result))
            end
        end
    end
end)

tab:Show()
